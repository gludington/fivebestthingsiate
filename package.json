{
  "name": "fivebestthingsiate",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "deploy": "npm run build && wrangler pages deploy dist  --project-name='fivebestthingsiate'",
    "db:migrate": "wrangler d1 execute DB --file=./migrations/0001_init.sql",
    "db:migrate:local": "wrangler d1 execute DB --local --file=./migrations/0001_init.sql"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "^11.1.0",
    "@astrojs/tailwind": "^5.1.2",
    "arctic": "^1.9.2",
    "astro": "^4.16.17",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "wrangler": "^4.62.0"
  }
}
